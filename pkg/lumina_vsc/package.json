{
    "name": "lumina-vsc",
    "displayName": "Lumina syntax for VS Code",
    "description": "Lumina syntax support for Visual Studio Code",
    "publisher": "lumina-lang",
    "license": "MIT",
    "version": "0.6.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/lloydlobo/lumina-lang/pkg/lumina-vsc"
    },
    "engines": {
        "vscode": "^1.75.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "Lumina",
        "lumina",
        "lum"
    ],
    "contributes": {
        "languages": [
            {
                "id": "lumina",
                "aliases": [
                    "Lumina",
                    "lumina",
                    "lum"
                ],
                "extensions": [
                    ".lum"
                ],
                "configuration": "language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "lumina",
                "scopeName": "source.lum",
                "path": "syntax/lumina.tmLanguage.json"
            }
        ]
    }
}
